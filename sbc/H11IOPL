#!/bin/sh
export BSD_TARGET_SBC=H11IOPL
export BSD_TARGET_SBC_ALIAS_LIST="ASUS_H11IO-PLUS skylake skylake-s" 
. $BSD_SOC/skylake
. $BSD_DEVICE/nic.em
. $BSD_DEVICE/nic.re
export BSD_KERNCONF_DEVICE_STO="$BSD_KERNCONF_DEVICE_STO nvme nvd ahci ata pass"
export BSD_KERNCONF_DEVICE_SND="$BSD_KERNCONF_DEVICE_SND snd_hda"
export BSD_KERNCONF_DEVICE_USB_CONTROLLER="$BSD_KERNCONF_DEVICE_USB_CONTROLLER xhci" 
