#!/bin/sh
. $BSD_ARCH/.all
. $BSD_ARCH/.64bit
. $BSD_ARCH/.efi
. $BSD_ARCH/.acpi
. $BSD_ARCH/.fdt
export TARCH_L1=arm64
export GOARCH=arm64
export GOBUILDMODE=default
export BSD_KERNCONF_CPU=ARM64
export BSD_KERNCONF_DEVICE="$BSD_KERNCONF_DEVICE arm_doorbell scmi mem"
export BSD_KERNCONF_DEVICE="$BSD_KERNCONF_DEVICE clk phy cpufreq vt kbdmux vt_efifb syscon firmware mmio_sram pci pwm"
export BSD_KERNCONF_OPTIONS="$BSD_KERNCONF_OPTIONS SMP SCHED_ULE NUMA PREEMPTION VIMAGE EFIRT" 
export BSD_KERNCONF_OPTIONS="$BSD_KERNCONF_OPTIONS SCSI_DELAY=5000" 
# export BSD_KERNCONF_OPTIONS="$BSD_KERNCONF_OPTIONS SMP SCHED_4BSD" # optional SCHED_ULE 
export BSD_KERNCONF_OPTIONS="$BSD_KERNCONF_OPTIONS FDT VFP INTRNG PERTHREAD_SSP"
export BSD_KERNCONF_OPTIONS="$BSD_KERNCONF_OPTIONS LINUX_BOOT_ABI USB_HOST_ALIGN=64"
