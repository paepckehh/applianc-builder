#!/bin/sh
export BSD_TARGET_SBC=P8P67DE
export BSD_TARGET_SBC_ALIAS_LIST="ASUS_P8P67-DELUXE ASUS_P8P67 sandybridge"
. $BSD_SOC/sandybridge
. $BSD_DEVICE/nic.em
. $BSD_DEVICE/nic.re
export NIC_PRIMARY="em0"
export NIC_SECONDARY="re0"
export BSD_KERNCONF_DEVICE_STO="$BSD_KERNCONF_DEVICE_STO nvme nvd ahci ata pass"
export BSD_KERNCONF_DEVICE_SND="$BSD_KERNCONF_DEVICE_SND snd_hda"
export BSD_KERNCONF_DEVICE_USB_CONTROLLER="$BSD_KERNCONF_DEVICE_USB_CONTROLLER ehci ohci uhci xhci" 
